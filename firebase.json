{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "python310",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "target": "gestion",
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "gestioncontrol"
      }
    ]
  }
}